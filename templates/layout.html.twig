<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="{{ asset('css/style.css') }}">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">
    <title>{% block title %} Document {% endblock %}</title>
</head>
<body>
{% for value in app.session.flashBag.get('welcome') %}
    <div class="alert alert-info">
        {{ value }}
    </div>
{% endfor %}
{% for value in app.session.flashBag.get('warning') %}
    <div class="alert alert-warning">
        {{ value }}
    </div>
{% endfor %}
{% for value in app.session.flashBag.get('success') %}
    <div class="alert alert-success">
        {{ value }}
    </div>
{% endfor %}
{% for value in app.session.flashBag.get('error') %}
    <div class="alert alert-warning">
        {{ value }}
    </div>
{% endfor %}{% for value in app.session.flashBag.get('erreur') %}
    <div class="alert alert-warning">
        {{ value }}
    </div>
{% endfor %}

{% block body %}

{% endblock %}
</body>
</html>